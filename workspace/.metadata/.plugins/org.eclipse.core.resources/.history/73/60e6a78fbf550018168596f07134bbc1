void manualControl(){
	while()
	if (getch() == '\033') {
	    getch();
	    switch(getch()) {
	        case 'A':
	        	printfln("up");
	            break;
	        case 'B':
	        	printfln("up");
	            break;
	        case 'C':
	        	printfln("up");
	            break;
	        case 'D':
	        	printfln("up");
	            break;
	    }
	}
}
