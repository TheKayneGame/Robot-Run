#include "kayneLib.h"

void manualControl() {
	printf("inpunt234234234");
	int ch;
	while (TRUE) {
		ch = getch();
		if (ch == 0) {
			/* This is a special key. Get the second byte */
			ch = getch();
			printf("The second byte is %d\n", ch);
		}
	}


