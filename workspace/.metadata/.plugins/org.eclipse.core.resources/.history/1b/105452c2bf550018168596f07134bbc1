#include "kayneLib.h"

void manualControl(){
	while(TRUE)
	if (getch() == '\033') {
	    getch();
	    switch(getch()) {
	        case 'A':
	        	printfln("up");
	            break;
	        case 'B':
	        	printfln("up");
	            break;
	        case 'C':
	        	printfln("up");
	            break;
	        case 'D':
	        	printfln("up");
	            break;
	    }
	}

	if (getch() == 'E'){
		break;
	}
}
